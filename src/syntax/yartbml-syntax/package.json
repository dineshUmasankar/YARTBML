{
  "name": "yartbml-syntax",
  "displayName": "yartbml-syntax",
  "description": "syntax highlighter for YARTBML",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "ybml",
      "aliases": ["YARTBML", "ybml"],
      "extensions": [".ybml"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "ybml",
      "scopeName": "source.ybml",
      "path": "./syntaxes/ybml.tmLanguage.json"
    }]
  }
}
