{
  "name": "yartbml",
  "displayName": "YARTBML",
  "description": "YARTBML language support for VSCode",
  "version": "0.0.1",
  "publisher": "YBML",
  "author": {
    "name": "Dinesh Umasankar",
    "email": "dinesh71uma@gmail.com"
  },
  "icon": "assets/logo.png",
  "engines": {
    "vscode": "^1.81.1"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ybml",
        "aliases": [
          "YARTBML",
          "ybml"
        ],
        "extensions": [
          ".ybml"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ybml",
        "scopeName": "source.ybml",
        "path": "./syntaxes/ybml.tmLanguage.json"
      }
    ]
  },
