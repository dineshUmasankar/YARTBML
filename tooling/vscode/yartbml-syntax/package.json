{
  "name": "yartbml",
  "displayName": "YARTBML",
  "description": "YARTBML syntax highlighter",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.1"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ybml",
        "aliases": [
          "YARTBML",
          "ybml"
        ],
        "extensions": [
          ".ybml"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "dark": "../../../assets/logo.png",
          "light": "../../../assets/logo.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "ybml",
        "scopeName": "source.ybml",
        "path": "./syntaxes/ybml.tmLanguage.json"
      }
    ]
  }
}
