{
  "name": "yartbml",
  "displayName": "YARTBML",
  "description": "YARTBML syntax highlighter",
  "version": "0.0.4",
  "publisher": "Ourselves",
  "icon": "assets/logo.png",
  "engines": {
    "vscode": "^1.81.1"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ybml",
        "aliases": [
          "YARTBML",
          "ybml"
        ],
        "extensions": [
          ".ybml"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ybml",
        "scopeName": "source.ybml",
        "path": "./syntaxes/ybml.tmLanguage.json"
      }
    ]
  },
  "dependencies": {
    "vsce": "^2.15.0"
  }
}
